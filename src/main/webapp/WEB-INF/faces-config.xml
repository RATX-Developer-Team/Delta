<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>beanRegister</managed-bean-name>
        <managed-bean-class>com.daw.delta.beans.register.beanRegister</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/logreg.jsp</from-view-id>
        <navigation-case>
            <from-outcome>volver</from-outcome>
            <to-view-id>/logreg.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/index.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>beanLogin</managed-bean-name>
        <managed-bean-class>com.daw.delta.beans.login.beanLogin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <validator>
        <validator-id>emailValidador</validator-id>
        <validator-class>com.daw.delta.validator.emailValidator</validator-class>
   </validator>
    
    <navigation-rule>
        <from-view-id>/index.jsp</from-view-id>
        <navigation-case>
            <from-outcome>alta</from-outcome>
            <to-view-id>/logreg.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/articulo.jsp</from-view-id>
        <navigation-case>
            <from-outcome>irArticulo</from-outcome>
            <to-view-id>/articulo.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/logregAntiguo.jsp</from-view-id>
        <navigation-case>
            <from-outcome>alta</from-outcome>
            <to-view-id>/logreg.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/categoria.jsp</from-view-id>
        <navigation-case>
            <from-outcome>alta</from-outcome>
            <to-view-id>/logreg.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/subcategoria.jsp</from-view-id>
        <navigation-case>
            <from-outcome>alta</from-outcome>
            <to-view-id>/logreg.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
